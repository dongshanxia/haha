<script type="text/javascript">
	window.onload = function(){ 
	$("#teac_top").load("${proPath}/app/teac_top.action");
		
	    $.ajax({
	        url: "${proPath}/app/teac_select.action",
	        type: "POST",
	        dataType: "json",
	        //data: { "ID": row["ID"] },
	        success: function (res) {
	  
	        	        
	        	var json = eval(res.contactResult); //数组 	      	        	
	             $.each(json, function (index, item) {  
	                 //循环获取数据    
	                 var contactPhone = json[index].contactPhone;  
	                 var contactAddress = json[index].contactAddress;  
	                 var contactEmail = json[index].contactEmail;
	                 var contactPost = json[index].contactPost;
	                 
	                 $("#contact").html($("#contact").html()+ 
	                		 "<div> &nbsp;<strong>号码:</strong>" + contactPhone  + "</div>"+
	                		 "<div> &nbsp;<strong>地址:</strong>" + contactAddress + "</div>"+
	                		 "<div> &nbsp;<strong>邮箱:</strong>" + contactEmail +  "</div>"+
	                		 "<div> &nbsp;<strong>船政:</strong>" + contactPost +  "</div>"+
	                		 "<div> &nbsp;<strong></strong>" +   "</div>"
	                 );  
	             });
	        	
	             var json2 = eval(res.educationResult); //数组 	      	        	
	             $.each(json2, function (index, item) {  
	                 //循环获取数据    
	                 var contactPhone = json2[index].contactPhone;  
	                 var contactAddress = json2[index].contactAddress;  
	                 var contactEmail = json22[index].contactEmail;
	                 var contactPost = json2[index].contactPost;
	                 
	                 $("#contact").html($("#contact").html()+ 
	                		 "<div> &nbsp;<strong>号码:</strong>" + contactPhone  + "</div>"+
	                		 "<div> &nbsp;<strong>地址:</strong>" + contactAddress + "</div>"+
	                		 "<div> &nbsp;<strong>邮箱:</strong>" + contactEmail +  "</div>"+
	                		 "<div> &nbsp;<strong>船政:</strong>" + contactPost +  "</div>"+
	                		 "<div> &nbsp;<strong></strong>" +   "</div>"
	                 );  
	             });
	        	
	        	
	        	
	        },
	        error: function (error) {
	            alert(JSON.stringify(error));
	        }
	    });	
		
		
	}

    </script>


